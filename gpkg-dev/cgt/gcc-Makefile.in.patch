--- src/gcc-13.2.0/gcc/Makefile.in	2023-08-20 17:41:59.317943376 +0300
+++ src/gcc-13.2.0/gcc/Makefile.in.patch	2023-08-20 17:46:01.877943283 +0300
@@ -530,13 +530,9 @@
 LINKER_PLUGIN_API_H = $(srcdir)/../include/plugin-api.h
 
 # Default native SYSTEM_HEADER_DIR, to be overridden by targets.
-NATIVE_SYSTEM_HEADER_DIR = @NATIVE_SYSTEM_HEADER_DIR@
+NATIVE_SYSTEM_HEADER_DIR = /data/data/com.termux/files/usr/glibc/include
 # Default cross SYSTEM_HEADER_DIR, to be overridden by targets.
-ifeq (@includedir@,$(prefix)/include)
-  CROSS_SYSTEM_HEADER_DIR = @CROSS_SYSTEM_HEADER_DIR@
-else
-  CROSS_SYSTEM_HEADER_DIR = @includedir@
-endif
+CROSS_SYSTEM_HEADER_DIR = /data/data/com.termux/files/usr/glibc/include
 
 # autoconf sets SYSTEM_HEADER_DIR to one of the above.
 # Purge it of unnecessary internal relative paths
@@ -689,7 +685,7 @@
 # Used in install-cross.
 gcc_tooldir = @gcc_tooldir@
 # Since gcc_tooldir does not exist at build-time, use -B$(build_tooldir)/bin/
-build_tooldir = $(exec_prefix)/$(target_noncanonical)
+build_tooldir = /data/data/com.termux/files/usr/glibc
 # Directory in which the compiler finds target-independent g++ includes.
 gcc_gxx_include_dir = @gcc_gxx_include_dir@
 gcc_gxx_include_dir_add_sysroot = @gcc_gxx_include_dir_add_sysroot@
